{
          "Command": "/usr/bin/python -m SimpleHTTPServer",
          "Endpoints": [{
              "Name": "websvc",
              "Application": "websvc",
              "PortNumber": 8000,
              "Protocol": "tcp",
              "Purpose": "export",
              "VHosts": ["websvc"]
            }
          ],
          "ConfigFiles": {
                "/foo/bar.txt": {
                    "Filename": "/foo/bar.txt",
                    "Owner": "zenoss:zenoss",
                    "Permissions": "660"
                }
          },
          "ImageId": "ubuntu",
          "Instances": {
            "min": 1
          },
          "LogConfigs":[ {
            "path": "/tmp/foo",
            "type": "foo"
          }],
          "Snapshot": {
            "Pause": "echo pause",
            "Resume": "echo resume"
          }
}
